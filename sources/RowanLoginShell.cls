!RowanLoginShell methodsFor!

basicCaption

	^'Jadeite Login'.
!

fillVersionList
	| list |

	list := GciLibrary allSubclasses collect: [:each | each displayName].
	list := list reject: [:each | each isNil or:[('*3.2.15*' match: each) not]].
	list := list asSortedCollection: [:a :b | self isVersion: a lessThanOrEqualTo: b].
	versionListPresenter list: list!

loginFailureCaption
	^'Jadeite Login Error'!

postLogin: aGciSession
	| result |
	aGciSession verifyRowanLoaded ifFalse:[^aGciSession logout]. "Already notified the user"
	result := super postLogin: aGciSession.
	RowanTranscript showOnSession: aGciSession.
	^result !

updateCaption
	^'Jadeite Login'! !
